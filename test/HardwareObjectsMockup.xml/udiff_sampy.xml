    <channel type="epics" name="epicsMotor_rlv">IOC:m5.RLV</channel>
    <channel type="epics" name="epicsMotor_rbv" polling="500">IOC:m5.RBV</channel>
    <channel type="epics" name="epicsMotor_dmov" polling="500">IOC:m5.DMOV</channel>
    <channel type="epics" name="epicsMotor_stop">IOC:m5.STOP</channel>
    <channel type="epics" name="epicsMotor_velo">IOC:m5.VELO</channel>
    <channel type="epics" name="epicsMotor_dllm">IOC:m5.DLLM</channel>
    <channel type="epics" name="epicsMotor_dhlm">IOC:m5.DHLM</channel>
    <channel type="epics" name="epicsMotor_egu">IOC:m5.EGU</channel>
